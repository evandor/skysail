handleReadonly(field,i) ::= <<
  $handleLabel(field)$
  <div class='col-sm-10'>
    <span class='form-control' name='$field.id$' id='$field.id$' placeholder='$helpmessage(messages, field.placeholderKey)$' title="$helpmessage(messages, field.placeholderKey)$">$data.(field.id)$</span>
    <small class="help-block">$field.messageKey$: $helpmessage(messages, field.messageKey)$</small>
  </div>
>>